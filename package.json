{
  "name": "az-interface",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "audit:docs": "node scripts/audit-docs.cjs",
    "audit:technical": "node scripts/comprehensive-audit.cjs",
    "audit:config": "node scripts/audit-config.cjs",
    "audit:backlog": "node scripts/audit-backlog.cjs",
    "verify:internal-links": "markdown-link-check *.md",
    "verify:external-links": "markdown-link-check --config .markdown-link-check.json *.md",
    "check:broken-refs": "node scripts/check-broken-refs.js",
    "check:circular-deps": "madge --circular src/",
    "check:bundle-size": "webpack-bundle-analyzer dist/stats.json",
    "verify:dependencies": "npm audit && npm outdated",
    "verify:import-paths": "npm run type-check",
    "verify:config": "node scripts/verify-config.js",
    "check:permissions": "node scripts/check-permissions.js",
    "validate:access-controls": "node scripts/validate-access-controls.js",
    "validate:imports": "npm run type-check",
    "validate:module-resolution": "npm run build",
    "update:registry": "node scripts/update-registry.js",
    "generate:audit-report": "node scripts/generate-report.cjs",
    "audit:all": "npm run audit:docs && npm run audit:technical && npm run audit:config && npm run audit:backlog && npm run generate:audit-report",
    "generate:index": "node scripts/generate-index.cjs",
    "validate:index": "node scripts/validate-index.js",
    "create:component": "node scripts/create-component.js",
    "create:service": "node scripts/create-service.js",
    "define:integration": "node scripts/define-integration.js",
    "validate:system": "npm run validate:index && npm run type-check && npm run lint",
    "validate:imports": "node scripts/validate-imports.cjs",
    "validate:architecture": "node scripts/validate-architecture.cjs",
    "validate:app-registry": "node scripts/validate-app-registry.cjs",
    "validate:all": "npm run validate:imports && npm run validate:architecture && npm run validate:app-registry",
    "audit:index": "npm run generate:index && npm run validate:index",
    "optimize:build": "node scripts/build-optimizer.cjs",
    "optimize:implement": "node scripts/implement-optimizations.cjs",
    "optimize:all": "npm run generate:index && npm run audit:all && npm run optimize:build && npm run optimize:implement",
    "changelog:update": "node scripts/generate-changelog.cjs update",
    "changelog:release": "node scripts/generate-changelog.cjs release",
    "backup:create": "node scripts/rollback.cjs backup",
    "backup:list": "node scripts/rollback.cjs list",
    "backup:rollback": "node scripts/rollback.cjs rollback",
    "backup:validate": "node scripts/rollback.cjs validate",
    "version:control": "npm run changelog:update && npm run backup:create"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "npm run type-check",
      "npm run validate:index",
      "npm run validate:imports",
      "npm run validate:architecture",
      "npm run validate:app-registry"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ],
    "*.md": [
      "npm run audit:docs",
      "npm run verify:internal-links"
    ],
    "package.json": [
      "npm run verify:dependencies",
      "npm run update:registry",
      "npm run generate:index"
    ]
  },
  "dependencies": {
    "@google/genai": "^1.12.0",
    "arktype": "^2.1.20",
    "lucide-react": "^0.263.1",
    "monaco-editor": "^0.52.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.4"
  }
}
